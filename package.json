{
  "name": "django-tailwind-htmx",
  "version": "1.0.0",
  "scripts": {
    "htmx": "cp ./node_modules/htmx.org/dist/htmx.min.js ./src/static/js/htmx.min.js",
    "dev": "npm run htmx && tailwindcss -i ./src/static/css/input.css -o ./src/static/css/style.css --watch",
    "build": "npm run htmx && tailwindcss -o ./src/static/css/style.css --minify"
  },
  "devDependencies": {
    "daisyui": "^4.12.2",
    "tailwindcss": "^3.4.4"
  },
  "dependencies": {
    "htmx.org": "^1.9.12"
  }
}